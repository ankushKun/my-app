# Required: OpenRouter API key (get one at https://openrouter.ai/keys)
OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxx

# Required: Telegram bot token (create via @BotFather on Telegram)
TELEGRAM_BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrSTUvwxYZ

# Optional: Comma-separated Telegram usernames allowed to chat (default: * = everyone)
TELEGRAM_ALLOWED_USERS=*

# Optional: Model to use (default: anthropic/claude-sonnet-4)
NULLCLAW_MODEL=anthropic/claude-sonnet-4
